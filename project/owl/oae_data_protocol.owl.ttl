@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix oae_data_protocol: <https://w3id.org/clevinson/oae-data-protocol/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

oae_data_protocol:ProjectCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ProjectCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty oae_data_protocol:entries ],
        [ a owl:Restriction ;
            owl:allValuesFrom oae_data_protocol:Project ;
            owl:onProperty oae_data_protocol:entries ] ;
    skos:definition "A holder for Project objects" ;
    skos:inScheme <https://w3id.org/clevinson/oae-data-protocol> .

oae_data_protocol:NamedThing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NamedThing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty oae_data_protocol:id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty oae_data_protocol:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty oae_data_protocol:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty oae_data_protocol:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty oae_data_protocol:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uriorcurie ;
            owl:onProperty oae_data_protocol:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty oae_data_protocol:name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty oae_data_protocol:description ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty oae_data_protocol:name ] ;
    skos:definition "A generic grouping for any identifiable entity" ;
    skos:exactMatch schema1:Thing ;
    skos:inScheme <https://w3id.org/clevinson/oae-data-protocol> .

oae_data_protocol:Project a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Project" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty oae_data_protocol:primary_email ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty oae_data_protocol:age_in_years ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty oae_data_protocol:birth_date ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Date ;
            owl:onProperty oae_data_protocol:birth_date ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty oae_data_protocol:vital_status ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty oae_data_protocol:primary_email ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty oae_data_protocol:age_in_years ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:onDatatype xsd:string ;
                    owl:withRestrictions ( [ xsd:pattern "^\\S+@[\\S+\\.]+\\S+" ] ) ] ;
            owl:onProperty oae_data_protocol:primary_email ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty oae_data_protocol:age_in_years ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty oae_data_protocol:birth_date ],
        [ a owl:Restriction ;
            owl:allValuesFrom oae_data_protocol:PersonStatus ;
            owl:onProperty oae_data_protocol:vital_status ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty oae_data_protocol:vital_status ],
        oae_data_protocol:NamedThing ;
    skos:definition "Represents a Project" ;
    skos:inScheme <https://w3id.org/clevinson/oae-data-protocol> .

PATO:0001421 a owl:Class,
        oae_data_protocol:PersonStatus ;
    rdfs:label "ALIVE" ;
    rdfs:subClassOf oae_data_protocol:PersonStatus .

PATO:0001422 a owl:Class,
        oae_data_protocol:PersonStatus ;
    rdfs:label "DEAD" ;
    rdfs:subClassOf oae_data_protocol:PersonStatus .

<https://w3id.org/clevinson/oae-data-protocol/PersonStatus#UNKNOWN> a owl:Class,
        oae_data_protocol:PersonStatus ;
    rdfs:label "UNKNOWN" ;
    rdfs:subClassOf oae_data_protocol:PersonStatus .

oae_data_protocol:entries a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "entries" ;
    skos:inScheme <https://w3id.org/clevinson/oae-data-protocol> .

oae_data_protocol:age_in_years a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "age_in_years" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of years since birth" ;
    skos:inScheme <https://w3id.org/clevinson/oae-data-protocol> .

oae_data_protocol:birth_date a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "birth_date" ;
    rdfs:range linkml:Date ;
    skos:definition "Date on which a person is born" ;
    skos:inScheme <https://w3id.org/clevinson/oae-data-protocol> .

oae_data_protocol:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    skos:definition "A human-readable description for a thing" ;
    skos:inScheme <https://w3id.org/clevinson/oae-data-protocol> .

oae_data_protocol:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A unique identifier for a thing" ;
    skos:inScheme <https://w3id.org/clevinson/oae-data-protocol> .

oae_data_protocol:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    skos:definition "A human-readable name for a thing" ;
    skos:inScheme <https://w3id.org/clevinson/oae-data-protocol> .

oae_data_protocol:primary_email a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "primary_email" ;
    skos:definition "The main email address of a person" ;
    skos:inScheme <https://w3id.org/clevinson/oae-data-protocol> .

oae_data_protocol:vital_status a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "vital_status" ;
    rdfs:range oae_data_protocol:PersonStatus ;
    skos:definition "living or dead status" ;
    skos:inScheme <https://w3id.org/clevinson/oae-data-protocol> .

oae_data_protocol:PersonStatus a owl:Class,
        linkml:EnumDefinition ;
    owl:unionOf ( PATO:0001421 PATO:0001422 <https://w3id.org/clevinson/oae-data-protocol/PersonStatus#UNKNOWN> ) ;
    linkml:permissible_values PATO:0001421,
        PATO:0001422,
        <https://w3id.org/clevinson/oae-data-protocol/PersonStatus#UNKNOWN> .

<https://w3id.org/clevinson/oae-data-protocol> a owl:Ontology ;
    rdfs:label "oae-data-protocol" ;
    dcterms:license "Apache Software License 2.0" ;
    dcterms:title "oae-data-protocol" ;
    rdfs:seeAlso <https://clevinson.github.io/oae-data-protocol> ;
    skos:definition "Base ontology and data schemas for ocean alkalinity enhancement projects, experiments, and field trails. This project aims to be a technical complement to Submarine Scientific's OAE Data Protocol, developed in conjunction with Carbon To Sea and NOAA." .

